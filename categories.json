[
  {
    "name": "API Failures",
    "matchedStatuses": ["failed"],
    "messageRegex": ".*failed.*"
  },
  {
    "name": "Timeout Issues",
    "matchedStatuses": ["failed"],
    "messageRegex": ".*timeout.*"
  },
  {
    "name": "Validation Errors",
    "matchedStatuses": ["failed"],
    "messageRegex": ".*assert.*"
  },
  {
    "name": "Server Errors",
    "matchedStatuses": ["failed"],
    "messageRegex": ".*(500|502|503).*"
  },
  {
    "name": "Authentication Errors",
    "matchedStatuses": ["failed"],
    "messageRegex": ".*(401|403).*"
  },
  {
    "name": "Not Found Errors",
    "matchedStatuses": ["failed"],
    "messageRegex": ".*404.*"
  },
  {
    "name": "Skipped Tests",
    "matchedStatuses": ["skipped"]
  }
]
